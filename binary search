#include<stdio.h>
void main()
{
int a[10];
int i,first,mid=0,last,el;
printf("Enter the array elements");
for(i=0;i<10;i++)
{
  scanf("%d",&a[i]);
}
printf("Enter the element to be searched");
scanf("%d",&el);
for(i=0;i<10;i++)
{
mid=(first+last)/2;
if(a[mid]==el)
{
printf("Element found in the array");
 exit(0);
}
else if(el>mid)
 first=mid+1;
else
 end=mid-1;
}
printf("Element not found in the array");
}
